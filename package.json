{
  "name": "MansalemaMobile",
  "version": "0.0.6",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --port 8082 && brew services start mongodb-community@8.0",
    "test": "jest",
    "clean-rebuild-android": "watchman watch-del-all && rm -rf yarn.lock package-lock.json node_modules android/app/build && npm install && npm start -- --reset-cache",
    "clean-rebuild-ios": "watchman watch-del-all && rm -rf yarn.lock package-lock.json node_modules ios/Pods ios/Podfile.lock android/app/build && npm install && cd ios && pod update && cd .. && npm start -- --reset-cache",
    "android:play": "cd android && ./gradlew installPlayDebug && adb shell am start -n com.maduane.salema/com.maduane.salema.MainActivity"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
